# Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# Solana RPC Endpoint (Required)
# Get free API key from: https://www.helius.dev/
VITE_SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY

# Magic Eden API (Optional, for enhanced features)
VITE_MAGIC_EDEN_API_KEY=

# CoinGecko API (For crypto prices)
VITE_COINGECKO_API_KEY=

# Development vs Production
NODE_ENV=development

# CORS Proxy (if using custom proxy)
VITE_PROXY_URL=

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
